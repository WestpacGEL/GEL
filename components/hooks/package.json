{
	"name": "@westpac/hooks",
	"version": "0.0.1",
	"description": "Hooks for the Westpac GEL",
	"repository": "https://github.com/WestpacGEL/GUI3.git",
	"author": "Westpac GEL Team <gel@westpac.com.au>",
	"license": "GPL-3.0+",
	"scripts": {
		"start": "PACKAGE_NAME=hooks webpack-dev-server --config ../../helpers/example/webpack.config.js",
		"build": "babel --root-mode upward src -d dist",
		"test": "start-server-and-test start http://localhost:8080 test:all",
		"test:dev": "start-server-and-test start http://localhost:8080 test:test-dev",
		"test:all": "bolt test:integration",
		"test:integration": "cypress run",
		"test:test-dev": "cypress open"
	},
	"main": "dist/index.js",
	"dependencies": {
		"@westpac/core": "^0.0.1",
		"react": "^16.7.0-alpha.1",
		"resize-observer-polyfill": "^1.5.0"
	},
	"devDependencies": {
		"cypress": "^3.1.3",
		"start-server-and-test": "^1.7.11"
	}
}
