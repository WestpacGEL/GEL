{
	"name": "@westpac/list-group",
	"version": "0.0.1",
	"description": "List groups are a flexible and powerful component for displaying not only simple lists of elements, but complex ones with custom content. Ideal for settings pages or preferences.",
	"repository": "https://github.com/WestpacGEL/GEL.git",
	"author": "Westpac GEL Team <gel@westpac.com.au>",
	"license": "GPL-3.0+",
	"scripts": {
		"start": "PACKAGE_NAME=list-group webpack-dev-server --config ../../helpers/example/webpack.config.js",
		"build": "babel --root-mode upward src -d dist",
		"test": "start-server-and-test start http://localhost:8080 test:all",
		"test:dev": "start-server-and-test start http://localhost:8080 test:test-dev",
		"test:all": "bolt test:integration",
		"test:integration": "cypress run",
		"test:test-dev": "cypress open"
	},
	"main": "dist/index.js",
	"dependencies": {
		"@westpac/core": "^0.0.1",
		"prop-types": "^15.6.2"
	},
	"peerDependencies": {
		"react": "16.7.0-alpha.2"
	},
	"devDependencies": {
		"cypress": "^3.1.3",
		"start-server-and-test": "^1.7.11"
	}
}
