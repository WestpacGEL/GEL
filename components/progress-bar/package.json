{
	"name": "@westpac/progress-bar",
	"version": "0.0.1",
	"description": "A visual indication of progress. Use when loading content or to indicate how far along the user is in a journey.",
	"repository": "https://github.com/WestpacGEL/GEL.git",
	"author": "Westpac GEL Team <gel@westpac.com.au>",
	"license": "GPL-3.0+",
	"scripts": {
		"start": "PACKAGE_NAME=progress-bar webpack-dev-server --config ../../helpers/example/webpack.config.js",
		"test": "start-server-and-test start http://localhost:8080 test:all",
		"test:dev": "start-server-and-test start http://localhost:8080 test:test-dev",
		"test:all": "yarn test:integration",
		"test:integration": "cypress run",
		"test:test-dev": "cypress open"
	},
	"main": "dist/progress-bar.cjs.js",
	"module": "dist/progress-bar.esm.js",
	"dependencies": {
		"@babel/runtime": "^7.6.2",
		"@westpac/a11y": "^0.0.1",
		"@westpac/core": "^0.0.1",
		"prop-types": "^15.7.2"
	},
	"peerDependencies": {
		"react": "^16.11.0"
	},
	"devDependencies": {
		"cypress": "^3.6.0",
		"react": "^16.11.0",
		"start-server-and-test": "^1.10.6"
	}
}
